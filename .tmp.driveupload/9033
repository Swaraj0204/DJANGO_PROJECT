#include<iostream>
using namespace std;
int main() {
   char grade='a';              //type conversion small->big
  int val=grade;
  double price=101.9999;        //type casting big->small
  int newprice=(int)price;
  int age;
  cout<< "enter your age:";
  cin>> age;                    //input from user 
  cout<< "your age is  "   <<age;
   
  int a,b;
  cout<< "enter a:";
  cin>>a;
  cout<<"enter b:";
cin>>b;
int sum=a+b;/
cout<<"the sum is "<<sum;
return 0;
}
