#include<iostream>
using namespace std;

  int main(){
    int marks;
    cout<<"enter marks:";
    cin>>marks;
     if(marks>=90){
     cout<<"a grade";
     } else if(marks>=80 && marks<=90){
      cout<<"b grade";
     } else if(marks>=60 && marks<=80){
     cout<<"c grade";}
     else {
      cout<<"fail";
     }

 

    return 0;
  }